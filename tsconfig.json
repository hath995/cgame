{
    "compilerOptions": {
        "module": "commonjs",
        "sourceMap": true,
        "target": "es2016",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "outDir": "./dist/",
        "jsx": "react"

    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src/*",
        "test/*"
    ]
}
